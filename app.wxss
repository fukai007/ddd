.askRoot{
  height: 100vh;
  background-image: url(https://wxapp.haizeihuang.com/wannengdequan_php/images/background_B.png);
  background-repeat: no-repeat;
  background-size: cover;
}

.questionArea{
  background: #fff;
  border-radius: 16rpx;
  height: 730rpx;
}

.baseInfo{
  height: 80rpx;
  margin-left: 70rpx;
  margin-right: 70rpx;
}

.userIcon{
  width: 80rpx;
  height: 80rpx;
  border: 2rpx solid #fff;
  border-radius: 80rpx;
}
