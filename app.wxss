.obar{
  position: absolute;
  bottom: 0;
  height: 100rpx;
  width: 100%;
  background-color: #f2f2f2;
}

.obarItem{
  width: 80rpx;
}

.root3d{
  perspective: 800px;
}

.cube{
  transform-style: preserve-3d;
}

.levelItem{
  transition:transform 500ms ease 0ms;
}
.levelItem:active{
  transform:rotateX(25deg);
  transform-origin:50% 50% 0px;
}


